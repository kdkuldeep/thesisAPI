{
  "extends": ["airbnb-base", "prettier", "plugin:security/recommended"],
  "plugins": ["prettier", "security"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "linebreak-style": 0,
    "consistent-return": 0,
    "camelcase": 0,
    "no-console": 0
  }
}
